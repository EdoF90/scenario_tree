import yfinance as yf
import pandas as pd
import numpy as np
import openpyxl
from scipy.stats import skew, kurtosis

def ExpectedMomentsEstimate(tickers):
    data = yf.download(tickers, start='2023-05-01', end='2024-05-01')['Adj Close']

    returns = data.pct_change().dropna()

    mean = returns.mean()
    std_dev = returns.std()
    skewness = returns.apply(lambda x: skew(x, bias=False))
    kurt = returns.apply(lambda x: kurtosis(x, bias=False, fisher=False))
    cor = returns.corr()

    return mean, std_dev, skewness, kurt, cor


